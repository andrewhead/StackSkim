<div id="answer-26318" class="answer accepted-answer" data-answerid="26318"  itemscope itemtype="http://schema.org/Answer" itemprop="acceptedAnswer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
    <input type="hidden" name="_id_" value="26318">
    <a class="vote-up-off" title="This answer is useful">up vote</a>
    <span itemprop="upvoteCount" class="vote-count-post ">75</span>
    <a class="vote-down-off" title="This answer is not useful">down vote</a>
    


