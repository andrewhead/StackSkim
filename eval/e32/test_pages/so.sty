    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #f90 #e0e0e0 #fff;
    border-image: none;
    border-left: 1px solid #e0e0e0;
    border-right: 1px solid #e0e0e0;
    border-style: solid;
    border-width: 2px 1px 1px;
    color: #222;
    font-weight: normal;
}
#tabs a:hover, .tabs a:hover {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #fff;
    border-color: #f90 #e0e0e0 #fff;
    border-image: none;
    border-left: 1px solid #e0e0e0;
    border-right: 1px solid #e0e0e0;
    border-style: solid;
    border-width: 2px 1px 1px;
}
.related {
    font-size: 12px;
}
.related a {
    font-size: 13px;
    font-weight: normal;
}
.linked {
    font-size: 12px;
    line-height: 1.3;
}
.linked a {
    font-size: 13px;
    font-weight: normal;
}
.label-key {
    font-size: 14px;
}
img.sponsor-tag-img {
    margin-bottom: -3px;
    margin-top: -1px;
}
#mainbar h2, .mainbar h2, #mainbar h3, .mainbar h3, #mainbar h4, .mainbar h4 {
    font-weight: normal;
}
.subtabs, .filter {
    margin: 8px 0;
    padding-right: 0;
}
.subtabs a, .filter a {
    color: #777;
    font-size: 12px;
    margin-right: 3px;
    padding: 4px 6px;
}
.subtabs a.youarehere, .filter a.youarehere, .subtabs a.active, .filter a.active {
    border-bottom: 1px solid #f90;
    color: #222;
    font-weight: normal;
}
.subtabs a:hover, .filter a:hover {
    color: #222;
}
#question-suggestions {
    width: 660px;
}
.narrow .status {
    height: auto;
    padding: 6px;
    width: 44px;
}
.narrow .mini-counts {
    font-size: 17px;
    margin-bottom: 4px;
}
.narrow .summary h3 {
    line-height: 1.3;
    margin-bottom: 0.35em;
}
.question-summary {
    border-bottom: 1px solid #e0e0e0;
    padding: 12px 0 10px;
}
.question-summary .answer-hyperlink, .question-summary .question-hyperlink {
    font-size: 16px;
}
.wmd-button > span {
    background-image: url("http://cdn.sstatic.net/stackoverflow/img/wmd-buttons.svg?v=eeae5e3efd60"), none;
    background-size: initial !important;
}
.wmd-container .wmd-button-bar .wmd-spacer2 {
    left: 195px !important;
}
.wmd-container .wmd-button-bar .wmd-spacer3 {
    left: 321px !important;
}
.user-show-new .wmd-container .wmd-button-bar .wmd-spacer2 {
    left: 173px !important;
}
.user-show-new .wmd-container .wmd-button-bar .wmd-spacer3 {
    left: 296px !important;
}
@media not all, not all, not all, not all {
.wmd-button > span {
    background-image: url("http://cdn.sstatic.net/stackoverflow/img/wmd-buttons.svg?v=eeae5e3efd60"), none;
}
}
.wmd-snippet-button span {
    background-image: url("http://cdn.sstatic.net/stackoverflow/img/wmd-buttons.svg?v=eeae5e3efd60"), none !important;
    background-position: -260px 0 !important;
}
.wmd-snippet-button span:hover {
    background-position: -260px -40px !important;
}
input[type="text"]:focus, input[type="email"]:focus, input[type="password"]:focus, textarea:focus {
    border: 1px solid #07c;
    outline: 0 none;
}
input[type="text"], textarea:not(.wmd-input), .tag-editor {
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
}
#hot-network-questions a {
    font-size: 12px;
}
.vote-up-off, .vote-up-on, .vote-down-off, .vote-down-on {
    height: 30px;
}
.vote-down-off, .vote-down-on {
    margin-bottom: 10px;
}
.star-on, .star-off {
    height: 30px;
}
textarea#wmd-input {
    font-family: Consolas,Menlo,Monaco,Lucida Console,Liberation Mono,DejaVu Sans Mono,Bitstream Vera Sans Mono,Courier New,monospace,serif;
}
.answered .viewcount {
    color: #fff;
}

