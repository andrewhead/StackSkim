&lt;pre&gt;&lt;code&gt;add_rewrite_rule(&#39;dealer-profile/([^/]+)&#39;, &#39;index.php?pagename=dealer- profile&amp;amp;dealer=$matches[1]&#39;, &#39;top&#39;);
