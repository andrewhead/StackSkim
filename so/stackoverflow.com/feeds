&lt;p&gt;&lt;strong&gt;Query&lt;/strong&gt;&lt;/p&gt;

&lt;pre&gt;&lt;code&gt;db.AggregateCollection.aggregate({ &quot;$group&quot; : { &quot;_id&quot; : { } , &quot;type1&quot; : { &quot;$sum&quot; : &quot;$actionType.1&quot;} , &quot;type2&quot; : { &quot;$sum&quot; : &quot;$actionType.2&quot;}}})
